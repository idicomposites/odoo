<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <report id="report_certificate_idi" 
        model="mrp.production" 
        string="Certificado de Calidad" 
        report_type="xlsx"
        name="as_idi_mrp.report_certificate_idi.xlsx" 
        file="as_idi_mrp.report_certificate_idi.xlsx"
        print_report_name="'Certificado - %s' % (object.name)" 
        attachment_use="False" 
        menu="True" 
        />
 <report id="as_materia_prima_qweb_pdf"
            model="stock.picking"
            string="Etiqueta materia prima (PDF)"
            name="as_idi_mrp.label_materia_prima_view_pdf"
            file="as_idi_mrp.label_materia_prima_view_pdf"
            report_type="qweb-pdf"
             menu="True" 
        />
        <record id="as_materia_prima_package" model="report.paperformat">
                <field name="name">Impresion Eqtiquetas Materia Prima</field>
                <field name="default" eval="True" />
                <field name="format">custom</field>
                <field name="page_height">150</field>
                <field name="page_width">100</field>
                <field name="margin_top">2</field>
                <field name="margin_bottom">2</field>
                <field name="margin_left">2</field>
                <field name="margin_right">2</field>
                <field name="orientation">Portrait</field>
                <field name="header_spacing">2</field>
                <field name="header_line" eval="False" />
                <field name="report_ids" eval="[(4, ref('as_idi_mrp.as_materia_prima_qweb_pdf'))]" />
        </record>
        <report 
        id="as_empaque_qweb_pdf" 
        model="stock.picking" 
        string="Lista de Empaque(PDF)"
        report_type="qweb-pdf"
        name="as_idi_mrp.empaque_view_pdf"
        file="as_idi_mrp.empaque_view_pdf"
        attachment_use="False" 
        menu="True" />
        <record id="as_empaque_package" model="report.paperformat">
                <field name="name">Impresion Lista de empaque</field>
                <field name="name">A4 Horizontal</field>
                <field name="default" eval="True"/>
                <field name="format">A4</field>
                <field name="orientation">Landscape</field>
                <field name="margin_top">30</field>
                <field name="margin_bottom">10</field>
                <field name="margin_right">4</field>
                <field name="margin_left">4</field>
                <field name="header_spacing">20</field>
                <field name="dpi">90</field>
                <field name="header_line" eval="False" />
                <field name="report_ids" eval="[(4, ref('as_idi_mrp.as_empaque_qweb_pdf'))]" />
        </record>
        

</odoo>