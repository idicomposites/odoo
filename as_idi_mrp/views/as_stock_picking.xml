<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="stock_picking_contenedor_form">
        <field name="name">stock.picking.contenedor.form</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="model">stock.picking</field>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='extra']" position="after">
                <page string="Contenedor" name="contenedor">
                    <field name="as_contenedor_id">
                        <tree string="Contenedor" editable="bottom" create="true" delete="false">
                            <field name="name" />
                            <field name="as_peso" />
                            <field name="as_lote" />
                        </tree>
                    </field>
                </page>
            </xpath>
        </field>
    </record>


</odoo>