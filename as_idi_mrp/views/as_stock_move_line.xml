<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="as_view_picking_tree_inherit_website_sale_stock" model="ir.ui.view">
        <field name="name">stock.move.line.operations.tree.inherit</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_stock_move_line_operation_tree" />
         <field name="priority">1001</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='location_dest_id']" position="after">
                <field name="as_lot_supplier" />
            </xpath>
      

        </field>
    </record>
    <record id="as_view_picking_form_inherit_website_sale_stock" model="ir.ui.view">
        <field name="name">stock.move.line.operations.form.inherit</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_move_line_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='location_dest_id']" position="after">
                <field name="as_lot_supplier" />
            </xpath>

        </field>
    </record>
</odoo>